// parameteriezd constuctor

#include<iostream>
using namespace std;

class xyz{
	private: int x,y;
	
	public:
		
		xyz(int x1, int y1) // parametriezd constructor
		{
			x=x1;
			y=y1;
		}
		
		 display_x1()
		{
			return x;
		}
		
		display_y1()
		{
			return y;
		}
		
};

int main()
{
	xyz s1(4 , 6);
	cout<<s1.display_x1();
	cout<<s1.display_y1();
}

#include<iostream>
using namespace std;

class emp{
	public: int num;
		
		emp() // default constructor
		{
			
		}
		
};

int main()

{
	emp e1;
	cout<<"number is 100:";
}

#include<iostream>

using namespace std;

class student{
	private:
		int a,b;
		
		public:
			student(int x, int y) // parameterized constructor
			{
				a=x;
				b=y;
			}
			
			student(student &ref) // copy constructor
			{
				a=ref.a;
				b=ref.b;
			}
			setdata() // method
			{
				cout<<a<<" "<<b<<endl;
			}
};

int main()
{
	student obj(4 , 5);
	student obj1=obj;
	obj.setdata();
	obj1.setdata();
}

