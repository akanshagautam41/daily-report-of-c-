	xyz e1;
}

#include<iostream>

using namespace std;
int main()
{
	int a[3][3],b[3][3],i,j,c[3][3];
	

	for(i=0;i<3;j++)
	{
		for(j=0;j<3;j++)
		cin>>a[i][j];
	}
	
	
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		cin>>b[i][j];
	
	}
	cout<<"\nEnter any value:";
	for(i=0; i<3; i++)
	{
		for(j=0; j<3; j++)
        c[i][j] = a[i][j]+b[i][j];
	}
	for(i=0;i<3;i++)
{
	for(j=0;j<3;j++)
	
	{
			for(j=0; j<5 ;j++)
		 cout<<c[i][j];
		}
}

}
 
	

// add two matrix program

#include<iostream>
using namespace std;

int main()
{
	int a[5][5] ,b[5][5] ,i,j, sum[5][5];
	int n;
	
	cout<<"enter any number:";
	cin>>n;
	for(i=0; i<5; i++)
	{
		for(j=0; j<5; j++)
		cin>>a[i][j];
	}
	cout<<"enter any number:";
	for(i=0; i<5; i++)
	{
		for(j=0; j<5; j++)
		cin>>b[i][j];
	}
	cout<<"\nEnter Total number of addition:";
	for(i=0; i<5; i++)
	{
		for(j=0; j<5; j++)
        sum[i][j] = a[i][j]+b[i][j];
	}
	cout<<"addition matrix: ";
	for(i=0; i<5; i++)
	{
		for(j=0; j<5 ;j++)
		 cout<<sum[i][j]<<" ";
        cout<<endl;
	}
	
	
	return 0;
}

#include<iostream>
using namespace std;

int main()
{
	int a,b,sum;
	
	cout<<"enter any number:";
	cin>>a>>b;
	
	sum=a+b;
	
	cout<<a<<"+"<<b<<"="<<sum;
	
	return 0;
	
	
}
